{
    "name": "graphql",
    "displayName": "GraphQL",
    "version": "0.1.0",
    "publisher": "mquandalle",
    "engines": {
        "vscode": "*"
    },
    "license": "MIT",
    "description": "Syntax highlighting for GraphQL queries and schemas",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "preview": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/apollostack/vscode-graphql.git"
    },
    "bugs": {
        "url": "https://github.com/apollostack/vscode-graphql/issues"
    },
    "icon": "images/graphql-logo.svg",
    "contributes": {
        "languages": [
            {
                "id": "graphql",
                "extensions": [
                    ".gql",
                    ".graphql"
                ],
                "aliases": [
                    "GraphQL",
                    "graphql"
                ],
                "configuration": "./graphql.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "graphql",
                "path": "./syntaxes/graphql.json",
                "scopeName": "source.graphql"
            }
        ],
        "snippets": [
            {
                "language": "graphql",
                "path": "./snippets/graphql.json"
            }
        ]
    }
}